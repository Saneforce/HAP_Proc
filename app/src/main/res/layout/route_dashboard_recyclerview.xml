<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/parent_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/C5dp"
        android:layout_marginRight="@dimen/C5dp"
        app:cardUseCompatPadding="true"
        app:cardCornerRadius="@dimen/C10dp"
        android:elevation="@dimen/C5dp">

        <LinearLayout
            android:id="@+id/cdParent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="@dimen/C5dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="@dimen/C20dp"
                        android:layout_height="@dimen/C20dp"
                        android:layout_gravity="center"
                        android:src="@drawable/ic_baseline_shopping_cart_24"
                        android:visibility="gone"/>
                    <LinearLayout
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:orientation="horizontal"
                        android:gravity="center_horizontal"
                        android:layout_gravity="center_horizontal"
                        android:background="@drawable/bugetoutletno">
                        <TextView
                            android:id="@+id/txRetNo"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="999"
                            android:paddingTop="@dimen/C4dp"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:textStyle="bold"
                            android:textSize="12sp"/>
                    </LinearLayout>
                    <TextView
                        android:id="@+id/retailername"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_weight="1"
                        android:ellipsize="end"
                        android:maxLines="1"
                        android:text="A.M.Snacks bar"
                        android:textColor="@color/black"
                        android:textSize="16dp"
                        android:textStyle="bold" />

                    <TextView
                        android:visibility="gone"
                        android:id="@+id/retailorCode"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/C10dp"
                        android:layout_weight="1"
                        android:text="167892"
                        android:textAlignment="textEnd"
                        android:textColor="@android:color/darker_gray"
                        android:textSize="14dp"/>
                    <LinearLayout
                        android:id="@+id/linDistance"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="@dimen/C15dp"
                            android:layout_height="@dimen/C15dp"
                            android:src="@drawable/ic_wlk_distance"/>
                        <TextView
                            android:id="@+id/txDistance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingLeft="@dimen/C2dp"
                            android:paddingRight="@dimen/C10dp"
                            android:paddingTop="2dp"
                            android:gravity="center_vertical"
                            android:textSize="10sp"
                            android:textStyle="bold"
                            android:text="10m"/>
                    </LinearLayout>
                    <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/round_status_blue">
                            <TextView
                                android:id="@+id/txChannel"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:paddingLeft="10dp"
                                android:paddingTop="2dp"
                                android:paddingRight="10dp"
                                android:textColor="@color/white"
                                android:textSize="10sp"
                                android:textStyle="bold"
                                android:text="DCO"/>
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-5dp"
                    android:layout_marginLeft="@dimen/C30dp">
                    <TextView
                        android:id="@+id/txOwnerNm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="12sp"/>
                    <LinearLayout
                        android:id="@+id/btnCallMob"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">                    <ImageView
                        android:id="@+id/icMob"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:src="@drawable/call_icon"
                        app:tint="@color/colorPrimaryDark"
                        />
                    <TextView
                        android:id="@+id/txMobile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="12sp"/>
                    </LinearLayout>

                </LinearLayout>
                <TextView
                    android:id="@+id/txAdd"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="9sp"
                    android:layout_marginTop="0dp"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey_200"
                    android:padding="@dimen/C5dp"
                    />
                <LinearLayout
                    android:id="@+id/llDataParent"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/C10dp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/llTdDataParent"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:visibility="visible">
                        <TextView
                            android:id="@+id/tvDate"
                            android:layout_width="match_parent"
                            android:layout_height="25dp"
                            android:fontFamily="sans-serif"
                            android:text="09-06-2021"
                            android:textSize="12sp"
                            android:gravity="center_vertical"
                            android:textStyle="bold"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:visibility="visible">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.5"
                                android:ellipsize="end"
                                android:fontFamily="sans-serif"
                                android:text=""
                                android:textSize="@dimen/C12dp"
                                android:textStyle="bold"/>
                            <TextView
                                android:layout_width="25dp"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:text="Ord"
                                android:textSize="10sp"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:text="|"
                                android:layout_marginLeft="@dimen/C2dp"
                                android:layout_marginRight="@dimen/C2dp"
                                android:textSize="12sp"/>
                            <TextView
                            android:layout_width="25dp"
                            android:layout_height="wrap_content"
                            android:fontFamily="sans-serif"
                            android:text="N.Q"
                            android:textSize="10sp"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:text="|"
                                android:layout_marginLeft="@dimen/C2dp"
                                android:layout_marginRight="@dimen/C2dp"
                                android:textSize="12sp"/>

                            <TextView
                                android:layout_width="25dp"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:text="Inv"
                                android:textSize="10sp"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:text="|"
                                android:layout_marginLeft="@dimen/C2dp"
                                android:layout_marginRight="@dimen/C2dp"
                                android:textSize="12sp"/>

                            <TextView
                                android:layout_width="70dp"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:textAlignment="textEnd"
                                android:text="Inv.Val"
                                android:textSize="10sp"/>

                        </LinearLayout>

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rvTodayData"
                            android:clickable="true"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            >
                        </androidx.recyclerview.widget.RecyclerView>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/grey_500"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:visibility="visible">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.5"
                                android:ellipsize="end"
                                android:fontFamily="sans-serif"
                                android:text="Total"
                                android:textSize="11sp"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/tvTodayTotOQty"
                                android:layout_width="25dp"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:text="0"
                                android:textSize="11sp"
                                android:textStyle="bold"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:text="|"
                                android:layout_marginLeft="@dimen/C2dp"
                                android:layout_marginRight="@dimen/C2dp"
                                android:textSize="12sp"/>
                            <TextView
                                android:id="@+id/tvTodayTotNOQty"
                                android:layout_width="25dp"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:text="0"
                                android:textSize="11sp"
                                android:textStyle="bold"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:text="|"
                                android:layout_marginLeft="@dimen/C2dp"
                                android:layout_marginRight="@dimen/C2dp"
                                android:textSize="12sp"/>
                            <TextView
                                android:id="@+id/tvTodayTotQty"
                                android:layout_width="25dp"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:text="0"
                                android:textSize="11sp"
                                android:textStyle="bold"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:text="|"
                                android:layout_marginLeft="@dimen/C2dp"
                                android:layout_marginRight="@dimen/C2dp"
                                android:textSize="12sp"/>

                            <TextView
                                android:id="@+id/tvTodayTotVal"
                                android:layout_width="70dp"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:textAlignment="textEnd"
                                android:text="₹0.00"
                                android:textSize="11sp"
                                android:textStyle="bold">


                            </TextView>


                        </LinearLayout>

                    </LinearLayout>
                    <LinearLayout
    android:layout_width="2dp"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/C5dp">
    <LinearLayout
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/grey_500"
        android:padding="@dimen/C5dp"
        android:layout_marginTop="@dimen/C30dp"
        android:layout_marginBottom="@dimen/C30dp"
        />
</LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="25dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="25dp"
                                android:ellipsize="end"
                                android:maxLines="1"
                                android:gravity="center_vertical"
                                android:text=""
                                android:textSize="12sp"
                                android:textStyle="bold"/>
                            <LinearLayout
                                android:layout_width="130dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/tvLMFirst"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="Sep"
                                    android:textAlignment="center"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:textSize="12sp"
                                    android:textStyle="bold"/>
                                <TextView
                                    android:id="@+id/tvLMSecond"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="Aug"
                                    android:textAlignment="center"
                                    android:textColor="@color/black_80"
                                    android:textSize="12sp"
                                    android:textStyle="normal"/>
                                <TextView
                                    android:id="@+id/tvLMThree"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:text="Jul"
                                    android:textAlignment="center"
                                    android:textColor="@color/black_80"
                                    android:textSize="12sp"
                                    android:textStyle="normal"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:visibility="visible">
                            <TextView
                                android:layout_width="40dp"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:text="Inv"
                                android:textSize="10sp"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:text="|"
                                android:layout_marginLeft="@dimen/C2dp"
                                android:layout_marginRight="@dimen/C2dp"
                                android:textSize="12sp"/>

                            <TextView
                                android:layout_width="80dp"
                                android:layout_height="wrap_content"
                                android:fontFamily="sans-serif"
                                android:textAlignment="textEnd"
                                android:text="Inv.Val"
                                android:textSize="10sp">


                            </TextView>


                        </LinearLayout>
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rvPreMnData"
                            android:clickable="false"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/grey_500"/>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:visibility="visible">

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.5"
                                    android:ellipsize="end"
                                    android:fontFamily="sans-serif"
                                    android:text="Total"
                                    android:textSize="@dimen/C12dp"
                                    android:textStyle="bold"
                                    android:visibility="gone"/>
                                <TextView
                                    android:id="@+id/tvPreTotQty"
                                    android:layout_width="@dimen/C40dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="sans-serif"
                                    android:text="0"
                                    android:textSize="11sp"
                                    android:textStyle="bold"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="sans-serif"
                                    android:text="|"
                                    android:layout_marginLeft="@dimen/C2dp"
                                    android:layout_marginRight="@dimen/C2dp"
                                    android:textSize="12sp"/>

                                <TextView
                                    android:id="@+id/tvPreTotVal"
                                    android:layout_width="80dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="sans-serif"
                                    android:textAlignment="textEnd"
                                    android:text="₹0.00"
                                    android:textSize="11sp"
                                    android:textStyle="bold">


                                </TextView>


                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="-5dp"
    >
    <TextView
        android:id="@+id/txDistName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/C10dp"
        android:textSize="10sp"
        android:textColor="@color/colorPrimaryDark"
        />
</LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@color/grey_200"
                    android:layout_marginBottom="@dimen/C2dp"/>
                <LinearLayout
                    android:layout_marginTop="@dimen/C2dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <LinearLayout
                        android:id="@+id/linDirection"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_gravity="center_vertical"
                        android:gravity="center_vertical">
                        <ImageView
                            android:layout_width="@dimen/C15dp"
                            android:layout_height="@dimen/C15dp"
                            android:src="@drawable/ic_directions"
                            app:tint="@color/colorPrimary"/>
                        <TextView
                            android:id="@+id/showDirection"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingLeft="@dimen/C2dp"
                            android:paddingRight="@dimen/C10dp"
                            android:paddingTop="2dp"
                            android:gravity="center_vertical"
                            android:textSize="11sp"
                            android:textStyle="bold"
                            android:text="Direction"
                            android:textColor="@color/colorPrimaryDark"
                            />
                    </LinearLayout>
                    <Space
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"/>
<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@color/txt_template_color"/>
</LinearLayout>
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="4dp"
                    android:layout_weight="1"
                    android:background="@drawable/bottom_border"
                    android:visibility="gone">

                </RelativeLayout>

            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone">
            <TextView
                android:id="@+id/status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/invoice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                />
        </LinearLayout>
    </androidx.cardview.widget.CardView>


